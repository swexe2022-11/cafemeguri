<h1>投稿ページ</h1>
<%= form_for @cafe, action: :create do |f| %>
<p>
  <%= f.label :name, '店名' %>
  <%= f.text_field :name %>
<br>
  <%= f.label :message, '説明' %>
  <%= f.text_area :message,:size=>"60x10" %>
<br>
  <%= f.label :image,'画像' %>
  <%= f.file_field :image %>
</p>
  <%= f.submit "投稿"%>
<% end %>
